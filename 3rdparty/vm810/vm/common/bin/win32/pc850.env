# This is the default environment file used until the language is defined.
# it corresponds to the "C" locale.  Variables are ordered the same way as
# in "setlocale".

# LC_MONETARY information

INT_CUR_SYM=FOO
CUR_SYM=$
MON_DEC_PNT=.
MON_THOUS=,
MON_GRP=3
POS_SGN=+
NEG_SGN=-
#INT_FRAC=2
#FRAC_DIG=2
P_CS_PRE=1
P_SEP_SP=0
N_CS_PRE=1
N_SEP_SP=0
P_SGN_POS=1
N_SGN_POS=1

# LC_NUMERIC information

DEC_PNT=.
THOUS_SEP=
GROUPING=3

# LC_TIME information

NLTIME=%H:%M:%S
NLDATE=%m/%d/%y
NLLDATE=%b %d %Y
NLDATIM=%b %d %Y %H:%M:%S
NLSDAY=Sun:Mon:Tue:Wed:Thu:Fri:Sat
NLLDAY=Sunday:Monday:Tuesday:Wednesday:Thursday:Friday:Saturday
NLSMONTH=Jan:Feb:Mar:Apr:May:Jun:Jul:Aug:Sep:Oct:Nov:Dec
NLLMONTH=January:February:March:April:May:June:July:August:September:October:November:December
NLYEAR=19890108,Heisei:19261225,Showa:
NLTMISC=at:each:every:on:through:am:pm:zulu
NLTSTRS=now:yesterday:tomorrow:noon:midnight:next:weekdays:weekend:today
NLTUNITS=minute:minutes:hour:hours:day:days:week:weeks:month:months:year:years:min:mins
AMPMSTR=a.m.:p.m.

# LC_MESSAGES information

YESSTR=yes:y:Y
NOSTR=no:n:N
