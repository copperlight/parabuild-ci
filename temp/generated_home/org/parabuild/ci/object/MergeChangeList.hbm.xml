<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.MergeChangeList"
        table="MERGE_CHANGELIST"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <property
            name="mergeID"
            type="int"
            column="MERGE_ID"
            unique="false"
        />

        <property
            name="branchChangeListID"
            type="int"
            column="BRANCH_CHANGELIST_ID"
            unique="false"
        />

        <property
            name="resultCode"
            type="byte"
            column="RESULT_CODE"
            unique="false"
        />

        <property
            name="mergeResultDescription"
            type="java.lang.String"
            column="RESULT_DESCRIPTION"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-MergeChangeList.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
