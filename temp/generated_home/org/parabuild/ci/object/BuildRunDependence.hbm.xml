<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildRunDependence"
        table="BUILD_RUN_DEPENDENCE"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <property
            name="dependentBuildRunID"
            type="int"
            column="DEPENDENT_BUILD_RUN_ID"
            unique="false"
        />

        <property
            name="leadingBuildRunID"
            type="int"
            column="LEADER_BUILD_RUN_ID"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildRunDependence.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
