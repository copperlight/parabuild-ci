<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.PromotionStepDependency"
        table="PROMOTION_STEP_DEPENDENCY"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <property
            name="promotionStepID"
            type="int"
            column="PROMOTION_STEP_ID"
            unique="false"
        />

        <property
            name="dependencyPromotionStepID"
            type="int"
            column="DEPENDENCY_PROMOTION_STEP_ID"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PromotionStepDependency.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
