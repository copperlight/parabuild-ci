<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildRunParticipant"
        table="BUILD_RUN_PARTICIPANT"
    >
        <cache usage="read-write" />

        <id
            name="participantID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <property
            name="buildRunID"
            type="int"
            column="BUILD_RUN_ID"
            unique="false"
        />

        <property
            name="changeListID"
            type="int"
            column="CHANGELIST_ID"
            unique="false"
        />

        <property
            name="firstBuildRunID"
            type="int"
            column="FIRST_BUILD_RUN_ID"
            unique="false"
        />

        <property
            name="firstBuildRunNumber"
            type="int"
            column="FIRST_BUILD_RUN_NUMBER"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildRunParticipant.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
