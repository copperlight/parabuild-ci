<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildRunAction"
        table="BUILD_RUN_ACTION"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <property
            name="buildRunID"
            type="int"
            column="BUILD_RUN_ID"
            unique="false"
        />

        <property
            name="code"
            type="byte"
            column="CODE"
            unique="false"
        />

        <property
            name="action"
            type="java.lang.String"
            column="ACTION"
            unique="false"
        />

        <property
            name="date"
            type="java.util.Date"
            column="ACTION_DATE"
            unique="false"
        />

        <property
            name="description"
            type="java.lang.String"
            column="DESCRIPTION"
            unique="false"
        />

        <property
            name="userID"
            type="int"
            column="USER_ID"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildRunAction.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
