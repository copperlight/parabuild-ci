<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.StartParameter"
        table="MANUAL_RUN_PARAMETER"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <version
            name="timeStamp"
            type="long"
            column="TIMESTAMP"
        />

        <property
            name="buildID"
            type="int"
            column="BUILD_ID"
            unique="false"
        />

        <property
            name="name"
            type="java.lang.String"
            column="NAME"
            unique="true"
        />

        <property
            name="enabled"
            type="yes_no"
            column="ENABLED"
            unique="false"
        />

        <property
            name="value"
            type="java.lang.String"
            column="VALUE"
            unique="false"
        />

        <property
            name="runtimeValue"
            type="java.lang.String"
            column="RUNTIME_VALUE"
            unique="false"
        />

        <property
            name="presentation"
            type="byte"
            column="PRESENTATION"
            unique="false"
        />

        <property
            name="type"
            type="byte"
            column="TYPE"
            unique="false"
        />

        <property
            name="description"
            type="java.lang.String"
            column="DESCRIPTION"
            unique="false"
        />

        <property
            name="required"
            type="yes_no"
            column="REQUIRED"
            unique="false"
        />

        <property
            name="modifiable"
            type="yes_no"
            column="MODIFIABLE"
            unique="false"
        />

        <property
            name="order"
            type="int"
            column="DISPLAY_ORDER"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-StartParameter.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
