<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildRun"
        table="BUILD_RUN"
    >
        <cache usage="read-write" />

        <id
            name="buildRunID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <version
            name="timeStamp"
            type="long"
            column="TIMESTAMP"
        />

        <property
            name="buildID"
            type="int"
            column="BUILD_ID"
            unique="false"
        />

        <property
            name="activeBuildID"
            type="int"
            column="ACTIVE_BUILD_ID"
            unique="false"
        />

        <property
            name="buildRunNumber"
            type="int"
            column="NUMBER"
            unique="false"
        />

        <property
            name="resultID"
            type="byte"
            column="RESULT"
            unique="false"
        />

        <property
            name="resultDescription"
            type="java.lang.String"
            column="RESULT_DESCRIPTION"
            unique="false"
        />

        <property
            name="complete"
            type="byte"
            column="COMPLETE"
            unique="false"
        />

        <property
            name="finishedAt"
            type="java.util.Date"
            column="FINISHED_AT"
            unique="false"
        />

        <property
            name="startedAt"
            type="java.util.Date"
            column="STARTED_AT"
            unique="false"
        />

        <property
            name="buildName"
            type="java.lang.String"
            column="BUILD_NAME"
            unique="false"
        />

        <property
            name="syncNote"
            type="java.lang.String"
            column="SYNC_NOTE"
            unique="false"
        />

        <property
            name="label"
            type="java.lang.String"
            column="LABEL"
            unique="false"
        />

        <property
            name="changeListNumber"
            type="java.lang.String"
            column="CHANGELIST_NUM"
            unique="false"
        />

        <property
            name="lastStepRunName"
            type="java.lang.String"
            column="LAST_STEP_NAME"
            unique="false"
        />

        <property
            name="labelStatus"
            type="byte"
            column="LABEL_STATUS"
            unique="false"
        />

        <property
            name="manualLabel"
            type="java.lang.String"
            column="MANUAL_LABEL"
            unique="false"
        />

        <property
            name="reRun"
            type="yes_no"
            column="RERUN"
            unique="false"
        />

        <property
            name="dependence"
            type="byte"
            column="DEPENDENCE"
            unique="false"
        />

        <property
            name="type"
            type="byte"
            column="TYPE"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildRun.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
