<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildWatcher"
        table="BUILD_WATCHER"
    >
        <cache usage="read-write" />

        <id
            name="watcherID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <version
            name="timeStamp"
            type="long"
            column="TIMESTAMP"
        />

        <property
            name="buildID"
            type="int"
            column="BUILD_ID"
            unique="false"
        />

        <property
            name="email"
            type="java.lang.String"
            column="EMAIL"
            unique="true"
        />

        <property
            name="level"
            type="byte"
            column="LEVEL"
            unique="true"
        />

        <property
            name="disabled"
            type="yes_no"
            column="DISABLED"
            unique="false"
        />

        <property
            name="instantMessengerAddress"
            type="java.lang.String"
            column="IM_ADDRESS"
            unique="false"
        />

        <property
            name="instantMessengerType"
            type="byte"
            column="IM_TYPE"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildWatcher.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
