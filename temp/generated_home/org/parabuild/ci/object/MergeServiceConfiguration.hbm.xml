<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.MergeServiceConfiguration"
        table="MERGE_SERVICE_CONFIGURATION"
    >
        <cache usage="read-write" />

        <id
            name="ID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="assigned">
            </generator>
        </id>

        <version
            name="timeStamp"
            type="long"
            column="TIMESTAMP"
        />

        <property
            name="projectID"
            type="int"
            column="PROJECT_ID"
            unique="false"
        />

        <property
            name="startupMode"
            type="byte"
            column="STARTUP_MODE"
            unique="false"
        />

        <property
            name="deleted"
            type="yes_no"
            column="DELETED"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-MergeServiceConfiguration.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
