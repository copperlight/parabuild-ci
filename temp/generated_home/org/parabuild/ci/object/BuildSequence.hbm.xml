<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 1.1//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="org.parabuild.ci.object.BuildSequence"
        table="BUILD_SEQUENCE"
    >
        <cache usage="read-write" />

        <id
            name="sequenceID"
            column="ID"
            type="int"
            unsaved-value="-1"
        >
            <generator class="identity">
            </generator>
        </id>

        <version
            name="timeStamp"
            type="long"
            column="TIMESTAMP"
        />

        <property
            name="buildID"
            type="int"
            column="BUILD_ID"
            unique="false"
        />

        <property
            name="stepName"
            type="java.lang.String"
            column="NAME"
            unique="true"
        />

        <property
            name="scriptText"
            type="java.lang.String"
            column="SCRIPT"
            unique="false"
        />

        <property
            name="successPatterns"
            type="java.lang.String"
            column="SUCCESS_PATTERNS"
            unique="false"
        />

        <property
            name="failurePatterns"
            type="java.lang.String"
            column="FAILURE_PATTERNS"
            unique="false"
        />

        <property
            name="respectErrorCode"
            type="yes_no"
            column="RESPECT_ERROR_CODE"
            unique="false"
        />

        <property
            name="timeoutMins"
            type="int"
            column="TIMEOUT"
        />

        <property
            name="disabled"
            type="yes_no"
            column="DISABLED"
            unique="false"
        />

        <property
            name="continueOnFailure"
            type="yes_no"
            column="CONTINUE"
            unique="false"
        />

        <property
            name="finalizer"
            type="yes_no"
            column="FINALIZER"
            unique="false"
        />

        <property
            name="initializer"
            type="yes_no"
            column="INITIALIZER"
            unique="false"
        />

        <property
            name="lineNumber"
            type="int"
            column="LINE_NUMBER"
            unique="false"
        />

        <property
            name="type"
            type="byte"
            column="TYPE"
            unique="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BuildSequence.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
