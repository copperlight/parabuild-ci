              <action name="" id="122" beanClass="com.install4j.runtime.beans.actions.InstallFilesAction" enabled="true" commentSet="true" comment="" rollbackBarrier="false" multiExec="false" failureStrategy="1" errorMessage="">
                <serializedBean>
                  <java class="java.beans.XMLDecoder">
                    <object class="com.install4j.runtime.beans.actions.InstallFilesAction">
                      <void property="fileFilterScript">
                        <object class="com.install4j.api.beans.ScriptProperty">
                          <void property="value">
                            <string>// try {
                              final String path = fileInfo.getRelativeFilePath();
                              final String parabuildSh = "bin" + File.separator + "parabuild.sh";
                              final String parabuildVmoptions = "bin" + File.separator + "parabuild.vmoptions";
                              final String serverXml = "etc" + File.separator + "conf" + File.separator + "server.xml";
                              if (path.equals(parabuildSh) || path.equals(parabuildVmoptions)) {
                              // Check if the file already exists
                              final File file = fileInfo.getDestinationFile();
                              if (!file.exists()) {
                              return true;
                              }

                              // Read into string
                              final StringBuffer sb = new StringBuffer(300);
                              BufferedReader br = null;
                              try {
                              br = new BufferedReader(new InputStreamReader(new FileInputStream(file)));
                              String ln = br.readLine();
                              while (ln != null) {
                              sb.append(ln).append('\n');
                              ln = br.readLine();
                              }
                              } finally {
                              if (br != null) {
                              try {
                              br.close();
                              } catch (IOException ignore) {
                              }
                              }
                              }

                              // Find matchs
                              final java.util.regex.Pattern pattern = java.util.regex.Pattern.compile("-Xmx([0-9]+)m", java.util.regex.Pattern.CASE_INSENSITIVE);
                              final java.util.regex.Matcher matcher = pattern.matcher(sb);
                              while (matcher.find()) {
                              // add to set of key to look up in the DB
                              final String maxHeapMegabytes = matcher.group(1);
                              final int heap = Integer.parseInt(maxHeapMegabytes);
                              if (heap &lt; 300) {
                              return true;
                              }
                              }

                              // Did not find the mx don't install.
                              return false;
                              } if (path.equals(serverXml)) {
                              // Install if does not exist
                              if (!context.getDestinationFile(serverXml).exists()) {
                              return true;
                              }
                              // Read properties and see if this is an agent
                              final String parabuildConf = "etc" + File.separator + "system" + File.separator + "parabuild.conf";
                              final File parabuildConfFile = context.getDestinationFile(parabuildConf);
                              if (!parabuildConfFile.exists()) {
                              return true;
                              }
                              FileInputStream fis = null;
                              try {
                              final Properties props = new Properties();
                              fis = new FileInputStream(parabuildConfFile);
                              props.load(fis);
                              if (props.getProperty("parabuild.build.manager.ipaddress", "").trim().length() == 0) {
                              // Not an agent, don't overwrite
                              return false;
                              } else {
                              // Agent, overwrite
                              return true;
                              }
                              } catch (Exception e) {
                              // Don't overwrite by defau;t
                              return false;
                              } finally {
                              if (fis != null) {
                              try {
                              fis.close();
                              } catch (IOException ignore) {
                              }
                              }
                              }
                              } else {
                              return true;
                              }
                              // } catch (Exception e) {
                              // Always overwrite in case of error.
                              // return true;
                              // }
                            </string>
                          </void>
                        </object>
                      </void>
                    </object>
                  </java>
